{
	"manifest_version": 3,
	"name": "Search Port",
	"version": "0.0.5",
	"description": "A modern, customizable homepage with quick search options",
	"permissions": ["storage"],
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"16": "icons/16x.png",
		"48": "icons/48x.png",
		"128": "icons/128x.png"
	},

	"browser_specific_settings": {
		"gecko": {
			"id": "searchport@whirlxd.xyz",
			"strict_min_version": "91.0"
		}
	}
}
